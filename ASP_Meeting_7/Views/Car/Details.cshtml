@model Car
<dl>
    <dt>Модель:</dt>
    <dd>@Model.ModelName</dd>
    <dt>Виробник:</dt>
    <dd>@Model.Manufacturer</dd>
    <dt>Ціна:</dt>
    <dd>@Model.Price, USD</dd>
</dl>
<a asp-action="Index"></a> | 
<a asp-action="Edit" asp-route-id="@Model.Id">Редагувати</a> | 
<a asp-action="Delete" asp-route-id="@Model.Id">Видалити</a>